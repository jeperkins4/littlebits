.panel.panel-default
  .panel-heading
    .panel-title
      Photo
      .pull-right
        = link_to raw(t('edit')), [:edit, photo], :class => 'btn btn-primary'
        = link_to raw(t('back',to: "Photos")), photos_path, :class => 'btn btn-default'
  .panel-body
    %dl
    %dt Name:
%dd= photo.name
    %dt Description:
%dd= photo.description
    %dt Priority:
%dd= photo.priority
    %dt Invention:
%dd= photo.invention
    %dt Media:
%dd= photo.media
    
